<%= form_for(@doctor) do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br />

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :practice_id %>
  <%= f.text_field :practice_id %>

   <%= f.submit %>

<% end %>
