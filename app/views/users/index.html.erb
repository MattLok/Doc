<div class="large-7 columns">
  <h3><%= "#{@practice.office_name} Doctors" %></h3>

    <% @doctors.each_slice(3) do |slice| %>

      <div class="row">
        <% slice.each do |doc| %>
          <div class="large-4 columns bor">
            <p>First Name: <%= doc.first_name %></p></br>
            <p>Last Name: <%= doc.last_name %></p></br>
            <p>Email: <%= doc.email %></p></br>
            <p>Permission: <%= doc.role %></p>
          </div>
        <% end %>
      </div>
    <% end %>

</div>
<div class=="large-3 columns">



</div>
