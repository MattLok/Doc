<h2>Sign up</h2>



<%= simple_form_for(resource, url: user_registration_path, method: :post) do |f| %>

  <h3>User Inputs</h3>

 
    <%= f.input :first_name %> 
    <%= f.input :last_name %> 
    <%= f.input :email %> 
    <%= f.input :password %> 
    <%= f.input :password_confirmation %> 
   

  <%= f.button :submit %>
<% end %> 

<%= render "devise/shared/links" %>
