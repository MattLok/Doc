<%= form_for(@practice) do |f| %>

  <%= f.label :office_name %>
  <%= f.text_field :office_name %><br />

  <%= f.label :contact_name %>
  <%= f.text_field :contact_name %><br />

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :email %>
  <%= f.text_field :email %>

   <%= f.submit %>

<% end %> 


